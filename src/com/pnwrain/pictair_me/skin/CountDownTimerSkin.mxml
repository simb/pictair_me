<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark"
	  minWidth="21" minHeight="21"
	  xmlns:mx="library://ns.adobe.com/flex/halo">

	<!-- host component -->
	<Metadata>
		[HostComponent("com.pnwrain.pictair_me.view.CountDownTimer")]
	</Metadata>
    
	<!-- states -->
	<s:states>
		<mx:State name="Base"/>
		<mx:State name="Three"/>
		<mx:State name="Two"/>
		<mx:State name="One"/>
	</s:states>
	<s:Rect height="30.3281" width="400"
		  includeIn="Base,Base,One,Three,Two"
		  verticalCenter="0">
		<s:fill>
			<s:SolidColor color="0xff0000"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke caps="none"
							  color="0xffffff"
							  joints="miter"
							  miterLimit="4"
							  weight="1"/>
		</s:stroke>
	</s:Rect>
	<s:Group includeIn="Base,One,Three,Two"
		   left="304">
		<s:Ellipse height="70" width="70"
				 left="0"
				 top="0">
			<s:fill>
				<s:SolidColor color="0xff0000"
							color.One="0xFFFFFF"/>
			</s:fill>
			<s:stroke>
				<s:SolidColorStroke caps="none"
								  color="0xffffff"
								  color.One="0xFF0000"
								  joints="miter"
								  miterLimit="4"
								  weight="2"/>
			</s:stroke>
		</s:Ellipse>
		<s:SimpleText color="0xffffff"
					 color.One="0xFF0000"
					 fontFamily="Arial"
					 fontSize="50"
					 fontWeight="bold"
					 horizontalCenter="2"
					 kerning="on"
					 lineHeight="120%"
					 text="1"
					 verticalCenter="2" />
	</s:Group>
	<s:Group includeIn="Base,One,Three,Two"
		   left="165">
		<s:Group left="0"
			   top="0">
			<s:Ellipse height="70.001" width="70.001"
					 left="0"
					 top="0">
				<s:fill>
					<s:SolidColor color="0xff0000"
								color.Two="0xFFFFFF"/>
				</s:fill>
				<s:stroke>
					<s:SolidColorStroke caps="none"
									  color="0xffffff"
									  color.Two="0xFF0000"
									  joints="miter"
									  miterLimit="4"
									  weight="2"/>
				</s:stroke>
			</s:Ellipse>
		</s:Group>
		<s:SimpleText color="0xffffff"
					 color.Two="0xFF0000"
					 fontFamily="Arial"
					 fontSize="50"
					 fontWeight="bold"
					 horizontalCenter="2"
					 kerning="on"
					 lineHeight="120%"
					 text="2"
					 verticalCenter="2" />
	</s:Group>
	<s:Group includeIn="Base,One,Three,Two"
		   left="25">
		<s:Ellipse height="70.001" width="70.001"
				 left="0"
				 top="0">
			<s:fill>
				<s:SolidColor color="0xff0000"
							color.Three="0xffffff"/>
			</s:fill>
			<s:stroke>
				<s:SolidColorStroke caps="none"
								  color="0xffffff"
								  color.Three="0xff0000"
								  joints="miter"
								  miterLimit="4"
								  weight="2"/>
			</s:stroke>
		</s:Ellipse>
		<s:SimpleText color="0xffffff"
					 color.Three="0xFF0000"
					 fontFamily="Arial"
					 fontSize="50"
					 fontWeight="bold"
					 horizontalCenter="2"
					 kerning="on"
					 lineHeight="120%"
					 text="3"
					 verticalCenter="2"
					 />
	</s:Group>
</s:Skin>